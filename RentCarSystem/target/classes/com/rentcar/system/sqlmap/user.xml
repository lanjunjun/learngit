<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rentcar.system.dao.UserDao">

	<select id="findUserById">
		select userID,userName,password,userType,roleID,branchID,userPhone,nation,address,idCard,gender,email,workunit from sys_user where userID=#{id}
	</select>
	
</mapper>